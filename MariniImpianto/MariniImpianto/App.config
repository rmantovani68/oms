          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/MariniImpianto/MariniImpiantoWCFService/" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
