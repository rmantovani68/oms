#!/bin/bash

echo cp ./src/DotNetMQ/bin/Debug/DotNetMQ.exe                                               ./binaries/DotNetMQ.exe                ; cp ./src/DotNetMQ/bin/Debug/DotNetMQ.exe                                               ./binaries/DotNetMQ.exe
echo cp ./src/MDSManager/bin/Debug/MDSManager.exe                                           ./binaries/MDSManager.exe              ; cp ./src/MDSManager/bin/Debug/MDSManager.exe                                           ./binaries/MDSManager.exe
echo cp ./src/Tools/MDSServiceProxyGenerator/bin/Debug/MDSServiceProxyGenerator.exe         ./binaries/MDSServiceProxyGenerator.exe; cp ./src/Tools/MDSServiceProxyGenerator/bin/Debug/MDSServiceProxyGenerator.exe         ./binaries/MDSServiceProxyGenerator.exe
echo cp ./src/DotNetMQ/bin/Debug/MDSCommonLib.dll                                           ./binaries/MDSCommonLib.dll            ; cp ./src/DotNetMQ/bin/Debug/MDSCommonLib.dll                                           ./binaries/MDSCommonLib.dll
echo cp ./src/DotNetMQ/bin/Debug/MDSCore.dll                                                ./binaries/MDSCore.dll                 ; cp ./src/DotNetMQ/bin/Debug/MDSCore.dll                                                ./binaries/MDSCore.dll
echo cp ./src/DotNetMQ/bin/Debug/Topshelf.dll                                               ./binaries/Topshelf.dll                ; cp ./src/DotNetMQ/bin/Debug/Topshelf.dll                                               ./binaries/Topshelf.dll
 
echo cp ./src/DotNetMQ/bin/Debug/DotNetMQ.exe                                               "/cygdrive/c/Program Files/DotNetMQ/DotNetMQ.exe"                ; cp ./src/DotNetMQ/bin/Debug/DotNetMQ.exe                                               "/cygdrive/c/Program Files/DotNetMQ/DotNetMQ.exe"
echo cp ./src/MDSManager/bin/Debug/MDSManager.exe                                           "/cygdrive/c/Program Files/DotNetMQ/MDSManager.exe"              ; cp ./src/MDSManager/bin/Debug/MDSManager.exe                                           "/cygdrive/c/Program Files/DotNetMQ/MDSManager.exe"
echo cp ./src/Tools/MDSServiceProxyGenerator/bin/Debug/MDSServiceProxyGenerator.exe         "/cygdrive/c/Program Files/DotNetMQ/MDSServiceProxyGenerator.exe"; cp ./src/Tools/MDSServiceProxyGenerator/bin/Debug/MDSServiceProxyGenerator.exe         "/cygdrive/c/Program Files/DotNetMQ/MDSServiceProxyGenerator.exe"
echo cp ./src/DotNetMQ/bin/Debug/MDSCommonLib.dll                                           "/cygdrive/c/Program Files/DotNetMQ/MDSCommonLib.dll"            ; cp ./src/DotNetMQ/bin/Debug/MDSCommonLib.dll                                           "/cygdrive/c/Program Files/DotNetMQ/MDSCommonLib.dll"
echo cp ./src/DotNetMQ/bin/Debug/MDSCore.dll                                                "/cygdrive/c/Program Files/DotNetMQ/MDSCore.dll"                 ; cp ./src/DotNetMQ/bin/Debug/MDSCore.dll                                                "/cygdrive/c/Program Files/DotNetMQ/MDSCore.dll"
echo cp ./src/DotNetMQ/bin/Debug/Topshelf.dll                                               "/cygdrive/c/Program Files/DotNetMQ/Topshelf.dll"                ; cp ./src/DotNetMQ/bin/Debug/Topshelf.dll                                               "/cygdrive/c/Program Files/DotNetMQ/Topshelf.dll"
 
